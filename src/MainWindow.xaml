<Window x:Class="BouncyCastleHashTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BouncyCastleHashTool"
        mc:Ignorable="d"
        Title="Bouncy Castle Hash Tool" MinWidth="987">
    <Window.Resources>
        <local:BoolToCharacterCasingConverter x:Key="BoolToCharacterCasingConverter"/>
        <local:LowerCaseToUpperCaseConverter x:Key="LowerCaseToUpperCaseConverter"/>
    </Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <GroupBox Header="File" Margin="10,10,10,5">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <TextBox x:Name="tbFileName" Margin="10,11,5,11" HorizontalAlignment="Stretch" />
                <Button x:Name="btnSelect" Content="..." Margin="5,10,10,10" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Stretch" MinWidth="12" MinHeight="20" Click="btnSelect_Click"/>
            </Grid>
        </GroupBox>
        <GroupBox Header="Cyclic Check" Grid.Row="1" Margin="10,5,10,5">
            <Grid>
                <StackPanel Orientation="Horizontal" Margin="10,10,10,10">
                    <CheckBox x:Name="cbCrc32" HorizontalAlignment="Left" Content="CRC32" Margin="5,0,5,0" />
                    <CheckBox x:Name="cbCrc64" HorizontalAlignment="Left" Content="CRC64" Margin="5,0,5,0" />
                </StackPanel>
            </Grid>
        </GroupBox>
        <GroupBox Header="Digest" Grid.Row="2" Margin="10,5,10,5">
            <ScrollViewer HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Disabled">
                <Grid x:Name="gridDigest">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <StackPanel Orientation="Horizontal" Margin="10,10,10,5">
                        <CheckBox x:Name="cbBlake2b" HorizontalAlignment="Left" Content="BLAKE2b" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbBlake2s" HorizontalAlignment="Left" Content="BLAKE2s" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbDstu7564_256" HorizontalAlignment="Left" Content="Купина-256 (DSTU 7564:2014)" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbDstu7564_384" HorizontalAlignment="Left" Content="Купина-384 (DSTU 7564:2014)" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbDstu7564_512" HorizontalAlignment="Left" Content="Купина-512 (DSTU 7564:2014)" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbGost3411" HorizontalAlignment="Left" Content="ГОСТ Р 34.11-94" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbKeccak" HorizontalAlignment="Left" Content="Keccak" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbMd2" HorizontalAlignment="Left" Content="MD2" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbMd4" HorizontalAlignment="Left" Content="MD4" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbMd5" HorizontalAlignment="Left" Content="MD5" Margin="5,0,5,0" VerticalAlignment="Center" IsChecked="True" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="10,5,10,10" Grid.Row="1">
                        <CheckBox x:Name="cbRipemd128" HorizontalAlignment="Left" Content="RIPEMD128" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbRipemd160" HorizontalAlignment="Left" Content="RIPEMD160" Margin="5,0,5,0" VerticalAlignment="Center" IsChecked="True" />
                        <CheckBox x:Name="cbRipemd256" HorizontalAlignment="Left" Content="RIPEMD256" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbRipemd320" HorizontalAlignment="Left" Content="RIPEMD320" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbSha1" HorizontalAlignment="Left" Content="SHA1" Margin="5,0,5,0" VerticalAlignment="Center" IsChecked="True" />
                        <CheckBox x:Name="cbSha224" HorizontalAlignment="Left" Content="SHA224" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbSha256" HorizontalAlignment="Left" Content="SHA256" Margin="5,0,5,0" VerticalAlignment="Center" IsChecked="True" />
                        <CheckBox x:Name="cbSha384" HorizontalAlignment="Left" Content="SHA384" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbSha512" HorizontalAlignment="Left" Content="SHA512" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbSha3" HorizontalAlignment="Left" Content="SHA3" Margin="5,0,5,0" VerticalAlignment="Center" IsChecked="True" />
                        <CheckBox x:Name="cbSm3" HorizontalAlignment="Left" Content="SM3" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbShake" HorizontalAlignment="Left" Content="Shake" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbTiger" HorizontalAlignment="Left" Content="Tiger" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <CheckBox x:Name="cbWhirlpool" HorizontalAlignment="Left" Content="Whirlpool" Margin="5,0,5,0" VerticalAlignment="Center" />
                        <Button x:Name="btnAllDigests" HorizontalAlignment="Left" Content="Select All" Margin="5,0,5,0" VerticalAlignment="Center" Padding="5,1,5,1" Click="btnAllDigests_Click" />
                    </StackPanel>
                </Grid>
            </ScrollViewer>
        </GroupBox>
